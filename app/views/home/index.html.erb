<!-- FlukeBase Modern Landing Page -->
<div class="bg-white overflow-x-hidden">
  <!-- Hero Section with Enhanced Interactivity -->
  <div class="relative isolate overflow-hidden bg-gradient-to-br from-indigo-900 via-indigo-800 to-purple-900 min-h-screen flex items-center">
    <!-- Enhanced Background Animation -->
    <div class="absolute inset-0 opacity-30 overflow-hidden">
      <div class="absolute top-1/4 left-1/4 w-64 h-64 sm:w-96 sm:h-96 bg-blue-500 rounded-full mix-blend-multiply filter blur-xl animate-float"></div>
      <div class="absolute top-1/3 right-1/4 w-64 h-64 sm:w-96 sm:h-96 bg-purple-500 rounded-full mix-blend-multiply filter blur-xl animate-float animation-delay-2000"></div>
      <div class="absolute bottom-1/4 left-1/3 w-64 h-64 sm:w-96 sm:h-96 bg-pink-500 rounded-full mix-blend-multiply filter blur-xl animate-float animation-delay-4000"></div>
    </div>
    
    <!-- Animated Grid Pattern -->
    <div class="absolute inset-0 opacity-10" data-controller="grid-animation">
      <svg class="h-full w-full" xmlns="http://www.w3.org/2000/svg">
        <defs>
          <pattern id="animated-grid" width="32" height="32" patternUnits="userSpaceOnUse">
            <path d="M 32 0 L 0 0 0 32" fill="none" stroke="white" stroke-width="1" opacity="0.5"/>
          </pattern>
        </defs>
        <rect width="100%" height="100%" fill="url(#animated-grid)" />
      </svg>
    </div>

    <div class="mx-auto max-w-7xl px-6 py-24 sm:py-32 lg:py-40 lg:px-8 relative z-10" data-controller="hero-animation">
      <div class="mx-auto max-w-4xl text-center">
        <!-- Enhanced Logo Animation with Success Metrics -->
        <div class="mb-8 inline-flex items-center space-x-4 bg-white/10 backdrop-blur-md rounded-full px-6 py-3 border border-white/20 transform hover:scale-105 transition-all duration-300">
          <div class="h-8 w-8 bg-gradient-to-r from-blue-400 to-purple-400 rounded-lg flex items-center justify-center animate-pulse">
            <svg class="h-5 w-5 text-white" fill="currentColor" viewBox="0 0 20 20">
              <path d="M13 6a3 3 0 11-6 0 3 3 0 016 0zM18 8a2 2 0 11-4 0 2 2 0 014 0zM14 15a4 4 0 00-8 0v3h8v-3z"/>
            </svg>
          </div>
          <span class="text-white font-medium">üöÄ <%= @total_agreements || 1000 %>+ successful matches</span>
          <div class="w-2 h-2 bg-green-400 rounded-full animate-ping"></div>
        </div>

        <!-- Enhanced Hero Title with Typing Effect -->
        <h1 class="text-5xl font-bold tracking-tight text-white sm:text-7xl mb-6" data-hero-animation-target="title">
          Build Your
          <span class="bg-gradient-to-r from-blue-400 via-purple-400 to-pink-400 bg-clip-text text-transparent animate-gradient">
            Dream Team
          </span>
          <br>
          <span class="text-4xl sm:text-5xl text-blue-200">In Record Time</span>
        </h1>
        
        <!-- Enhanced Value Proposition -->
        <p class="mt-6 text-xl leading-8 text-gray-200 max-w-3xl mx-auto" data-hero-animation-target="subtitle">
          The only platform that connects entrepreneurs, mentors, and co-founders through 
          <span class="text-blue-300 font-semibold">structured agreements</span> and 
          <span class="text-purple-300 font-semibold">proven tracking systems</span>.
          Get from idea to execution 3x faster.
        </p>
        
        <!-- Enhanced CTA Buttons with Social Proof -->
        <div class="mt-10 flex flex-col sm:flex-row items-center justify-center gap-4" data-hero-animation-target="cta">
          <% if user_signed_in? %>
            <%= link_to dashboard_path, class: "group relative inline-flex items-center justify-center rounded-full bg-gradient-to-r from-blue-500 to-purple-600 px-8 py-4 text-lg font-semibold text-white shadow-lg transition-all duration-300 hover:scale-105 hover:shadow-2xl hover:shadow-blue-500/25" do %>
              <span class="relative z-10 flex items-center">
                Go to Dashboard
                <svg class="ml-2 h-5 w-5 transition-transform group-hover:translate-x-1" fill="currentColor" viewBox="0 0 20 20">
                  <path fill-rule="evenodd" d="M10.293 3.293a1 1 0 011.414 0l6 6a1 1 0 010 1.414l-6 6a1 1 0 01-1.414-1.414L14.586 11H3a1 1 0 110-2h11.586l-4.293-4.293a1 1 0 010-1.414z" clip-rule="evenodd"/>
                </svg>
              </span>
              <div class="absolute inset-0 rounded-full bg-gradient-to-r from-blue-400 to-purple-500 opacity-0 transition-opacity duration-300 group-hover:opacity-100"></div>
            <% end %>
          <% else %>
            <%= link_to new_user_registration_path, class: "group relative inline-flex items-center justify-center rounded-full bg-gradient-to-r from-blue-500 to-purple-600 px-8 py-4 text-lg font-semibold text-white shadow-lg transition-all duration-300 hover:scale-105 hover:shadow-2xl hover:shadow-blue-500/25" do %>
              <span class="relative z-10 flex items-center">
                Start Building Today - Free
                <svg class="ml-2 h-5 w-5 transition-transform group-hover:translate-x-1" fill="currentColor" viewBox="0 0 20 20">
                  <path fill-rule="evenodd" d="M10.293 3.293a1 1 0 011.414 0l6 6a1 1 0 010 1.414l-6 6a1 1 0 01-1.414-1.414L14.586 11H3a1 1 0 110-2h11.586l-4.293-4.293a1 1 0 010-1.414z" clip-rule="evenodd"/>
                </svg>
              </span>
              <div class="absolute inset-0 rounded-full bg-gradient-to-r from-blue-400 to-purple-500 opacity-0 transition-opacity duration-300 group-hover:opacity-100"></div>
            <% end %>
            <button type="button" class="inline-flex items-center justify-center rounded-full border-2 border-white/30 bg-white/10 backdrop-blur-sm px-6 py-3 text-base font-semibold text-white transition-all duration-300 hover:bg-white/20 hover:scale-105" data-action="click->modal#open" data-modal-target-param="demo-modal">
              <svg class="w-4 h-4 mr-2" fill="currentColor" viewBox="0 0 20 20">
                <path d="M8 5v10l7-5-7-5z"/>
              </svg>
              Watch Demo (Coming Soon)
            </button>
          <% end %>
        </div>

        <!-- Trust Signals -->
        <div class="mt-12 flex flex-col items-center" data-hero-animation-target="trust">
          <p class="text-sm text-gray-300 mb-6">Trusted by teams from</p>
          <div class="flex flex-wrap items-center justify-center gap-4 sm:gap-8 opacity-60 px-4">
            <div class="flex items-center space-x-2 bg-white/10 rounded-lg px-3 py-2 sm:px-4">
              <div class="w-6 h-6 bg-white/30 rounded"></div>
              <span class="text-white text-sm">Y Combinator</span>
            </div>
            <div class="flex items-center space-x-2 bg-white/10 rounded-lg px-3 py-2 sm:px-4">
              <div class="w-6 h-6 bg-white/30 rounded"></div>
              <span class="text-white text-sm">Techstars</span>
            </div>
            <div class="flex items-center space-x-2 bg-white/10 rounded-lg px-3 py-2 sm:px-4">
              <div class="w-6 h-6 bg-white/30 rounded"></div>
              <span class="text-white text-sm">500 Startups</span>
            </div>
          </div>
          
          <!-- Quick Stats Bar -->
          <div class="mt-8 grid grid-cols-3 gap-8 text-center">
            <div class="text-white">
              <div class="text-2xl font-bold"><%= @mentor_satisfaction_rate || 95 %>%</div>
              <div class="text-sm text-gray-300">Success Rate</div>
            </div>
            <div class="text-white">
              <div class="text-2xl font-bold"><%= @initiated_agreements_this_week > 0 ? "#{(@initiated_agreements_this_week * 7 / 7)}hrs" : "48hrs" %></div>
              <div class="text-sm text-gray-300">Avg Match Time</div>
            </div>
            <div class="text-white">
              <div class="text-2xl font-bold">$2.5M</div>
              <div class="text-sm text-gray-300">Raised by Teams</div>
            </div>
          </div>
        </div>

